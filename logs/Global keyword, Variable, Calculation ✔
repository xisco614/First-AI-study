#Back to coding

print(2**2) #4
print(10%3) #1
print(5//3) #1
print(5 >= 5) #true
print(5 == 5) # true
print(3 + 7 == 10) # 3+7 = 10, True
print(3 != 10) #True
print(not(10 != 9)) #False
print((4 > 1) & (4 < 10)) #True
print((5 > 1) or (5 < 1)) # there is "or" so it is true

print(5 > 4 > 3) #true
print(5 > 3 > 10) #false
